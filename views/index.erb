<h1>Hogwarts Student List</h1>
<% @all_students.each do |student| %>
    <div class="student-list-item">
        <div class="student-crest-and-name">
            <img src=<%= student["url"] %> class="crest" />
            <p><b><%= student["first_name"] %> <%= student["second_name"] %></b> of the <i><%= student["house_name"] %></i> House</p>
        </div>
        <ul><li>Age: <%= student["age"] %></li></ul>
    </div>
<% end %>
